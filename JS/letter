    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>letters</title>
</head>
<body>
    <p id="demo"></p>
    <script>
        const letters = new Set();
        letters.add("a");
        letters.add("b");
        letters.add("c");
        letters.add(10);
        letters.add(11);
        letters.delete(10);
        // console.log(letters);
        document.getElementById("demo").innerHTML = Array.from(letters);
    </script>
    
</body>
</html>
